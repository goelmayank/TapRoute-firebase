{
  "rules": {
    "deals": {
      ".read": true,
      ".write": "auth != null"
    },
    "drivers": {
      ".read": true,
      ".write": false,
      "$userId": {
        ".read": "auth != null",
        ".write": "auth != null && auth.uid == $userId"
      }
    },
    "localities": {
      ".read": true,
      ".write": "auth != null"
    },
    "master_settings": {
      ".read": true,
      ".write": false
    },
    "users": {
      ".read": true,
      "$uid": {
        ".read": "auth != null",
        ".write": "auth != null && auth.uid == $uId"
      }
    },
    "followers": {
      "$followedUid": {
        "$followerUid": {
          ".read": "auth.uid === $followerUid",
          ".write": "auth.uid === $followerUid"
        }
      }
    },
    "transactions": {
      ".read": true,
      ".write": "auth != null"
    },
    "trips": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "location": {
      ".read": true,
      ".write": "auth != null"
    },
    "events": {
      ".read": true,
      ".write": "auth != null"
    },
    "journey": {
      ".read": true,
      ".write": "auth != null"
    },
    "metro": {
      ".read": true,
      ".write": "auth != null"
    },
    "base_fare": {
      ".read": true,
      ".write": "auth != null"
    },
    "$other": {
      ".read": "auth != null",
      ".write": false
    }
  }
}